{
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/socket.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/socket",
      "destination": "/api/socket.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/build/web/index.html"
    }
  ]
}
